\begin_layout Subsection
h
\end_layout

\begin_layout Standard
后面是 decuplet intermediate 部分。h i j 图产生自 local Lagrangian，而 k 图来自于 非定域理论的
 gauge link。
\begin_inset Formula $\Theta^{\mu\nu}$
\end_inset

是十重态部分拉氏量中的一个参数，
\begin_inset Formula 
\[
\Theta^{\mu\nu}=g^{\mu\nu}-\left(Z+\frac{1}{2}\right)\gamma^{\mu}\gamma^{\nu}
\]

\end_inset

我们最后会取
\begin_inset Formula $Z=\frac{1}{2}$
\end_inset

，所以
\begin_inset Formula $\Theta^{\mu\nu}=g^{\mu\nu}-\left(Z+\frac{1}{2}\right)\gamma^{\mu}\gamma^{\nu}=g^{\mu\nu}-\gamma^{\mu}\gamma^{\nu}=-\frac{1}{2}\left(\gamma^{\mu}\gamma^{\nu}-\gamma^{\nu}\gamma^{\mu}\right)=i\sigma^{\mu\nu}$
\end_inset

，
\begin_inset Formula $\Theta^{\mu\nu}=i\sigma^{\mu\nu}$
\end_inset

。
\begin_inset Newline newline
\end_inset

如果
\begin_inset Formula $\bar{\psi}$
\end_inset

是旋量，
\begin_inset Formula $\bar{\psi}\sigma^{\mu\nu}\psi$
\end_inset

是厄米的。一般来说，如果
\begin_inset Formula $A_{\mu\nu}^{\dagger}=A_{\mu\nu}$
\end_inset


\begin_inset Formula 
\[
\left(\bar{\psi}\sigma^{\mu\nu}\psi A_{\mu\nu}\right)^{\dagger}=A_{\mu\nu}^{\dagger}\psi^{\dagger}\left(-\frac{i}{2}\left(\gamma^{\nu\dagger}\gamma^{\mu\dagger}-\gamma^{\mu\dagger}\gamma^{\nu\dagger}\right)\right)\gamma^{0}\psi=\bar{\psi}\sigma^{\mu\nu}\psi A_{\mu\nu}
\]

\end_inset

所以
\begin_inset Formula $\bar{\psi}\Theta^{\mu\nu}\psi$
\end_inset

不是厄米的，它在厄米共轭下出一项负号。或者利用
\begin_inset Formula $\Theta^{\mu\nu}$
\end_inset

对于指标是反对称的，把这个负号囊括进交换指标顺序
\begin_inset Formula $\Theta^{\mu\nu}\to\Theta^{\nu\mu}$
\end_inset

，salamu 文章中即是如此。
\begin_inset Formula 
\begin{align*}
 & \mathcal{C}\left[\varepsilon^{ijk}\overline{T}_{\mu}^{ilm}\Theta^{\mu\nu}\left(u_{\nu}\right)^{lj}B^{mk}+\text{h.c.}\right]\\
\sim & \frac{C_{T\phi}}{f}\bar{T}_{\rho}\left(x\right)\partial_{\sigma}\phi\left(x\right)\Theta^{\rho\sigma}p\left(x\right)+\frac{C_{T\phi}}{f}\bar{p}\left(x\right)\Theta^{\alpha\beta}T_{\beta}\left(x\right)\partial_{\alpha}\phi\left(x\right)
\end{align*}

\end_inset

第二行是矩阵展开后的大概形式。这样两个场
\begin_inset Formula $\bar{T}_{\alpha}\left(x\right)T_{\sigma}\left(x\right)$
\end_inset

收缩，形成十重态传播子。类似于
\begin_inset Formula $\Theta_{\alpha\beta}$
\end_inset

，一般的十重态传播子依赖于 off–shell parameter 
\begin_inset Formula $Z$
\end_inset

。而物理不应该依赖于
\begin_inset Formula $Z$
\end_inset

，所以选取一种简单的形式，我们选取了
\begin_inset Formula $Z=\frac{1}{2}$
\end_inset

，传播子可以写成，
\begin_inset Formula 
\begin{align*}
P_{\beta\rho} & =\langle\hat{T}\,T_{\beta}\left(x\right)\bar{T}_{\rho}\left(x\right)\rangle=g_{\beta\rho}-\frac{1}{3}\gamma_{\beta}\gamma_{\rho}-\frac{\gamma_{\beta}p_{\rho}-\gamma_{\rho}p_{\beta}}{3M_{T}}-\frac{2p_{\beta}p_{\rho}}{3M_{T}^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
从拉氏量可以看到
\begin_inset Formula 
\begin{align*}
 & \mathcal{L}^{\text{nonlocal}}=\bar{B}\left(x\right)\left(i\gamma^{\mu}\mathcal{D}_{\mu,x}-M_{B}\right)B\left(x\right)+\bar{T}_{\mu}\left(x\right)\left(i\gamma^{\mu\nu\alpha}\mathcal{D}_{\alpha,x}-M_{T}\gamma^{\mu\nu}\right)T_{\nu}\left(x\right)+\mathcal{D}_{\mu,x}\phi\left(x\right);\left(\mathcal{D}_{\mu,x}\phi\left(x\right)\right)^{\dagger}\\
+ & \left[\frac{C_{B\phi}}{f}\bar{p}\left(x\right)\gamma^{\mu}\gamma^{5}B\left(x\right)+\frac{C_{T\phi}}{f}\bar{p}\left(x\right)\Theta^{\mu\nu}T_{\nu}\left(x\right)\right]{\color{red}\int_{a}F\left(a\right)\mathcal{G}_{\phi}^{q}\left(x,x+a\right)\mathcal{D}_{\mu,x+a}\phi\left(x+a\right)}+\text{H.c.}\\
+ & \frac{iC_{\phi\phi^{+}}}{2f^{2}}\bar{p}\left(x\right)\gamma^{\mu}p\left(x\right){\color{red}\int_{ab}F\left(a\right)F\left(b\right)\mathcal{G}_{\phi}^{q}\left(x+b,x+a\right)}{\color{red}\left[\phi\left(x+a\right)\left(\mathcal{D}_{\mu,x+b}\phi\left(x+b\right)\right)^{\dagger}-\mathcal{D}_{\mu,x+a}\phi\left(x+a\right);\phi\left(x+b\right)^{\dagger}\right]}\\
+ & \cdots
\end{align*}

\end_inset

十重态部分跟八重态部分有对应关系，
\begin_inset Formula 
\begin{align*}
\frac{C_{B\phi}}{f}\bar{p}\left(x\right)\gamma^{\mu}\gamma^{5}B\left(x\right) & \to\frac{C_{T\phi}}{f}\bar{p}\left(x\right)\Theta^{\mu\nu}T_{\nu}\left(x\right)\\
\frac{C_{B\phi}}{f}\bar{B}\left(x\right)\gamma^{\mu}\gamma^{5}p\left(x\right) & \to\frac{C_{T\phi}}{f}\bar{T}_{\nu}\left(x\right)\Theta^{\nu\mu}p\left(x\right)
\end{align*}

\end_inset

注意其中指标的改变，其他的推导过程都是不变的，所以在圈积分结果中直接做相应替换即可。
\begin_inset Formula 
\begin{align*}
\frac{C_{B\phi}}{f}\slashed{k}\gamma^{5} & \to\frac{C_{T\phi}}{f}\bar{p}\left(x\right)k_{\mu}\Theta^{\mu\nu}\text{ 末态}\\
\frac{C_{B\phi}}{f}\slashed{k}\gamma^{5}p\left(x\right) & \to\frac{C_{T\phi}}{f}\bar{T}_{\nu}\left(x\right)\Theta^{\nu\mu}k_{\mu}\text{ 初态}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename inset/inset_yu_loop_h.pdf

\end_inset


\end_layout

\begin_layout Standard
类似于图 a，图 h meson rainbow 的贡献为
\begin_inset Formula 
\begin{align*}
 & \Gamma_{h}^{\mu}\left(2\pi\right)^{4}\delta^{\left(4\right)}\left(p^{\prime}-p-q\right)=\bra{p^{\prime}}\int_{xyz}{\color{red}\left(i\right)}\mathcal{L}_{\text{had}\left(T\right)}^{\left(\text{nonloc}\right)}\left(x\right){\color{cyan}J_{q,\text{em}}^{\mu}\left(y\right)}{\color{red}\left(i\right)}\mathcal{L}_{\text{had}\left(T\right)}^{\left(\text{nonloc}\right)}\left(z\right)\ket{p}\\
= & {\color{red}\left(i\right)}^{2}\bra{p^{\prime}}\int_{xyzk}\frac{C_{T\phi}}{f}\bar{p}\left(x\right)\Theta^{\alpha\beta}T_{\beta}\left(x\right)\left[-i\left(k+q\right)_{\alpha}\right]\tilde{F}\left(k+q\right)\phi\left(x\right)\\
\times & {\color{cyan}\left(ie_{\phi}^{q}\right)\left[\phi^{\dagger}\left(y\right)\partial^{\mu}\phi\left(y\right)-\phi\left(y\right)\partial^{\mu}\phi^{\dagger}\left(y\right)\right]}\frac{C_{T\phi}}{f}\bar{T}_{\rho}\left(z\right)\Theta^{\rho\sigma}p\left(z\right)\left[+ik_{\sigma}\right]\tilde{F}\left(k\right)\phi^{\dagger}\left(x\right)\ket{p}\\
= & {\color{red}\left(i\right)^{2}}\left(ie_{\phi}^{q}\right)\frac{C_{T\phi}^{2}}{f^{2}}\int_{k}\tilde{F}\left(k+q\right)\tilde{F}\left(k\right)i\fmp_{\phi}\left(k+q\right)i\fmp_{\phi}\left(k\right){\color{red}\left(-i\right)\left(2k+q\right)^{\mu}}\\
\times & \bar{u}\left(p^{\prime}\right)\left(\left(k+q\right)_{\alpha}\Theta^{\alpha\beta}\right)i\fmp_{T}^{\beta\rho}\left(p-k\right)\left(\Theta^{\rho\sigma}k_{\sigma}\right)u\left(p\right)
\end{align*}

\end_inset

then,
\begin_inset Formula 
\begin{align*}
f_{h}^{+} & ={\color{red}\left(i\right)^{2}}\left(ie_{\phi}^{q}\right)\frac{C_{T\phi}^{2}}{f^{2}}\int_{k}\tilde{F}\left(k+q\right)\tilde{F}\left(k\right)i\fmp_{\phi}\left(k+q\right)i\fmp_{\phi}\left(k\right){\color{red}\left(-i\right)\left(2k+q\right)^{\mu}}\frac{M}{P^{+}}\delta\left(y-\frac{k^{+}}{p^{+}}\right)\\
 & \times\bar{u}\left(p^{\prime}\right)\left(\left(k+q\right)_{\alpha}\Theta^{\alpha\beta}\right)i\fmp_{T}^{\beta\rho}\left(p-k\right)\left(\Theta^{\rho\sigma}k_{\sigma}\right)u\left(p\right)
\end{align*}

\end_inset

where,
\begin_inset Formula 
\[
i\fmp_{T}^{\alpha\beta}\left(p\right)=\frac{-i\left(\slashed{p}+M_{T}\right)}{p^{2}-M_{T}^{2}}\left(g_{\alpha\beta}-\frac{1}{3}\gamma_{\alpha}\gamma_{\beta}-\frac{\gamma_{\alpha}p_{\beta}-\gamma_{\beta}p_{\alpha}}{3M_{T}}-\frac{2p_{\alpha}p_{\beta}}{3M_{T}^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
总结，
\begin_inset Formula 
\[
f_{h}^{+}=\left(ie_{\phi}^{q}\right)\frac{C_{T\phi}^{2}}{f^{2}}\int\frac{\dif^{4}k}{\left(2\pi\right)^{4}}\frac{M\overline{\Lambda}^{8}}{P^{+}}\delta\left(y-\frac{k^{+}}{p^{+}}\right)\bar{u}\left(p^{\prime}\right)H^{+}u\left(p\right)
\]

\end_inset

where
\begin_inset Formula 
\[
H^{\mu}=\frac{{\color{red}\left(2k+q\right)^{\mu}}\left(k+q\right)_{\alpha}\Theta^{\alpha\beta}S_{T}^{\beta\rho}\left(p-k\right)\Theta^{\rho\sigma}k_{\sigma}}{D_{\Lambda}\left(k+q\right)^{2}D_{\Lambda}\left(k\right)^{2}D_{\phi}\left(k+q\right)D_{\phi}\left(k\right)D_{T}\left(p-k\right)}
\]

\end_inset


\begin_inset Formula 
\[
iS_{T}^{\alpha\beta}\left(p\right)=-\left(\gamma p+M_{T}\right)\left(g_{\alpha\beta}-\frac{1}{3}\gamma_{\alpha}\gamma_{\beta}-\frac{\gamma_{\alpha}p_{\beta}-\gamma_{\beta}p_{\alpha}}{3M_{T}}-\frac{2p_{\alpha}p_{\beta}}{3M_{T}^{2}}\right)
\]

\end_inset

它是十重态传播子的旋量部分。
\end_layout

