\begin_inset Formula 
\begin{align*}
\mathcal{B}_{ijk} & =\frac{1}{\sqrt{6}}\left(\varepsilon_{ij\ell}B^{\ell k}+\varepsilon_{ik\ell}B^{\ell j}\right)\\
\bar{\mathcal{B}}_{ijk} & =\frac{1}{\sqrt{6}}\left(\varepsilon_{ij\ell}\bar{B}^{\ell k}+\varepsilon_{ik\ell}\bar{B}^{\ell j}\right)
\end{align*}

