\begin_layout Subsection
salamu 形式
\end_layout

\begin_layout Standard
代入到之前的非定域拉氏量中
\begin_inset Formula 
\begin{align*}
\mathcal{L}^{\text{nonlocal}} & =\bar{B}\left(x\right)\left(i\gamma^{\mu}\mathcal{D}_{\mu,x}-M_{B}\right)B\left(x\right)+\bar{T}_{\mu}\left(x\right)\left(i\gamma^{\mu\nu\alpha}\mathcal{D}_{\alpha,x}-M_{T}\gamma^{\mu\nu}\right)T_{\nu}\left(x\right)+\mathcal{D}_{\mu,x}\phi\left(x\right);\left(\mathcal{D}_{\mu,x}\phi\left(x\right)\right)^{\dagger}\\
 & +\frac{C_{B\phi}}{f}\left[\bar{p}\left(x\right)\gamma^{\mu}\gamma^{5}B\left(x\right)\int_{a}F\left(a\right){\color{blue}\mathcal{G}_{\phi}^{q}\left(x,x+a\right)\mathcal{D}_{\mu,x+a}}\phi\left(x+a\right)+\text{H.c.}\right]\\
 & +\frac{C_{T\phi}}{f}\left[\bar{p}\left(x\right)\Theta^{\mu\nu}T_{\nu}\left(x\right)\int_{a}F\left(a\right){\color{blue}\mathcal{G}_{\phi}^{q}\left(x,x+a\right)\mathcal{D}_{\mu,x+a}}\phi\left(x+a\right)+\text{H.c.}\right]\\
 & +\frac{iC_{\phi\phi^{+}}}{2f^{2}}\bar{p}\left(x\right)\gamma^{\mu}p\left(x\right)\int_{ab}F\left(a\right)F\left(b\right){\color{blue}\mathcal{G}_{\phi}^{q}\left(x+b,x+a\right)}\\
 & \times\left[\phi\left(x+a\right)\left({\color{blue}\mathcal{D}_{\mu,x+b}\phi\left(x+b\right)}\right)^{\dagger}-{\color{blue}\mathcal{D}_{\mu,x+a}\phi\left(x+a\right)};\phi^{\dagger}\left(x+b\right)\right]+\cdots
\end{align*}

\end_inset

其中
\begin_inset Formula 
\begin{align*}
\mathcal{G}_{\phi}^{q}\left(y,x\right) & =\exp\left[+ie_{\phi}^{q}\int_{x}^{y}\mathcal{A}_{\nu}\left(z\right)\dif z^{\nu}\right]=1+ie_{\phi}^{q}\int_{x}^{y}\mathcal{A}_{\nu}\left(z\right)\dif z^{\nu}\\
\mathcal{D}_{\mu,x+a}\phi\left(x+a\right) & =\left[\partial_{\mu,x+a}+\left(-ie_{\phi}^{q}\right)\mathcal{A}_{\mu}\left(x+a\right)\right]\phi\left(x+a\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
除了在He的论文中的形式，非定域拉氏量也可以用 salamu 形式 展开，
\begin_inset Formula 
\begin{align*}
\mathcal{G}_{\phi}^{q}\left(x+b,x+a\right) & =\exp\left[-ie_{\phi}^{q}\left(a-b\right)^{\mu}\int_{0}^{1}\dif t\mathcal{A}_{\mu}\left(x+at+b\left(1-t\right)\right)\right]\\
 & =1-ie_{\phi}^{q}\left(a-b\right)^{\mu}\int_{0}^{1}\dif t\mathcal{A}_{\mu}\left(x+at+b\left(1-t\right)\right)+\cdots\\
 & =1+\delta\mathcal{G}_{\phi}^{q}+\cdots\\
\text{特例：}\mathcal{G}_{\phi}^{q}\left(x,x+a\right) & =1+\left(-ie_{\phi}^{q}\right)a^{\mu}\int_{0}^{1}\dif t\mathcal{A}_{\mu}\left(x+at\right)
\end{align*}

\end_inset

其中作了积分变量平移，
\begin_inset Formula $z^{\mu}\to x^{\mu}+a^{\mu}t+b^{\mu}\left(1-t\right)$
\end_inset

，平移不改变积分测度，没有 Joccabi 因子，只改变积分上下限。总之
\begin_inset Formula 
\[
\delta\mathcal{G}_{\phi}^{q}=-ie_{\phi}^{q}\int_{x+b}^{x+a}\dif z^{\mu}A_{\mu}\left(z\right)=-ie_{\phi}^{q}\left(a-b\right)^{\mu}\int_{0}^{1}\dif t\mathcal{A}_{\mu}\left(x+at+b\left(1-t\right)\right)
\]

\end_inset


\begin_inset Formula $\mathcal{G}_{\phi}^{q}\mathcal{D}_{\mu,x}$
\end_inset

乘积展开的头几项是，
\begin_inset Formula 
\begin{align*}
\mathcal{G}_{\phi}^{q}\left(x,x+a\right)\mathcal{D}_{\mu,x}\phi\left(x+a\right) & =\partial_{\mu,x+a}\phi\left(x+a\right)\\
 & +\left(-ie_{\phi}^{q}\right)\mathcal{A}_{\mu}\left(x+a\right)\phi\left(x+a\right)\\
 & +\left(-ie_{\phi}^{q}\right)a^{\mu}\int_{0}^{1}\dif t\mathcal{A}_{\mu}\left(x+at\right)\partial_{\mu,x+a}\phi\left(x+a\right)
\end{align*}

\end_inset

类似地，
\begin_inset Formula 
\begin{align*}
 & {\color{blue}\mathcal{G}_{\phi}^{q}\left(x+b,x+a\right)}\left[\phi\left(x+a\right)\left(\mathcal{D}_{\mu,x+b}\phi\left(x+b\right)\right)^{\dagger}-\mathcal{D}_{\mu,x+a}\phi\left(x+a\right);\phi^{\dagger}\left(x+b\right)\right]\\
= & \int_{ab}\left[\phi\left(x+a\right)\partial_{\mu,x+b}\phi^{\dagger}\left(x+b\right)-\partial_{\mu,x+a}\phi\left(x+a\right);\phi^{\dagger}\left(x+b\right)\right]\\
+ & \int\dif^{4}a\int\dif^{4}b\left(ie_{\phi}^{q}\right)\left[\mathcal{A}_{\mu}\left(x+b\right)\phi\left(x+a\right)\phi^{\dagger}\left(x+b\right)+\mathcal{A}_{\mu}\left(x+a\right)\phi\left(x+a\right)\phi^{\dagger}\left(x+b\right)\right]\\
+ & {\color{blue}\left(-ie_{\phi}^{q}\right)\left(a-b\right)^{\mu}\int_{0}^{1}\dif t\mathcal{A}_{\mu}\left(x+at+b\left(1-t\right)\right)}\left[\phi\left(x+a\right)\partial_{\mu,x+b}\phi^{\dagger}\left(x+b\right)-\partial_{\mu,x+a}\phi\left(x+a\right);\phi^{\dagger}\left(x+b\right)\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
规范链接部分的拉氏量，
\begin_inset Formula 
\begin{align*}
\mathcal{L}_{\text{link}}^{\text{nonloc}}\left(x\right) & =\int_{0}^{1}\dif t\int_{a}\left(-ie_{\phi}^{q}\right)\bar{p}\left(x\right)\left[\frac{C_{B\phi}}{f}\gamma^{\rho}\gamma^{5}B\left(x\right)+\frac{C_{T\phi}}{f}\Theta^{\rho\nu}T_{\nu}\left(x\right)\right]F\left(a\right)a^{\mu}\mathcal{A}_{\mu}\left(x+at\right)\partial_{\rho}\phi\left(x+a\right)+\text{H.c}\\
 & +\int_{0}^{1}\dif t\int_{ab}\frac{e_{\phi}^{q}C_{\phi\phi^{\dagger}}}{2f^{2}}\bar{p}\left(x\right)\gamma^{\rho}p\left(x\right)F\left(a\right)F\left(b\right)\left(a-b\right)^{\mu}\mathcal{A}^{\mu}\left(x+at+b\left(1-t\right)\right)\\
 & \times\left[\phi\left(x+a\right)\partial_{\rho}\phi^{\dagger}\left(x+b\right)-\partial_{\rho}\phi\left(x+a\right);\phi\left(x+b\right)^{\dagger}\right]
\end{align*}

\end_inset

对应的额外流为
\begin_inset Formula 
\begin{align*}
\delta J_{q}^{\mu}\left(x\right) & \equiv\frac{\delta\int\dif^{4}y\mathcal{L}_{\text{link}}^{\text{nonloc}}\left(y\right)}{\delta\mathcal{A}_{\mu}\left(x\right)}\\
 & =\int_{0}^{1}\dif t\int_{a}F\left(a\right)\left(-ie_{\phi}^{q}\right)\bar{p}\left(x-at\right)\left[\frac{C_{B\phi}}{f}\gamma^{\rho}\gamma^{5}B\left(x-at\right)+\frac{C_{T\phi}}{f}\Theta^{\rho\nu}T_{\nu}\left(x-at\right)\right]a^{\mu}\partial_{\rho}\phi\left(x+a\left(1-t\right)\right)+\text{H.c}\\
 & +\int_{0}^{1}\dif t\int_{ab}\frac{iC_{\phi\phi^{\dagger}}}{2f^{2}}\left(-ie_{\phi}^{q}\right)F\left(a\right)F\left(b\right)\left(a-b\right)^{\mu}\bar{p}\left(x-at-b\left(1-t\right)\right)\gamma^{\rho}p\left(x-at-b\left(1-t\right)\right)\\
 & \times\left[\phi\left(x+\left(a-b\right)\left(1-t\right)\right)\partial_{\rho}\phi^{\dagger}\left(x-\left(a-b\right)t\right)-\partial_{\rho}\phi\left(x+\left(a-b\right)\left(1-t\right)\right);\phi\left(x-\left(a-b\right)t\right)^{\dagger}\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
terning 形式
\end_layout

\begin_layout Standard
通过积分变量平移，把
\begin_inset Formula $F\left(a\right)$
\end_inset

写成
\begin_inset Formula $F\left(x-y\right)$
\end_inset

 
\begin_inset Formula 
\begin{align*}
\mathcal{L}_{\text{had}}^{\text{nonloc}}\left(x\right) & =\bar{p}\left(x\right)\left[\frac{C_{B\phi}}{f}\gamma^{\mu}\gamma^{5}B\left(x\right)+\frac{C_{T\phi}}{f}\Theta^{\mu\nu}T_{\nu}\left(x\right)\right]\int\dif^{4}yF\left(y-x\right)\partial_{\mu}\phi\left(y\right)+\text{H.c.}\\
 & +\frac{iC_{\phi\phi^{\dagger}}}{f}\bar{p}\left(x\right)\gamma^{\mu}p\left(x\right)\int\dif^{4}y\int\dif^{4}zF\left(y-x\right)F\left(z-x\right)\left[\phi\left(y\right)\partial_{\mu}\phi^{\dagger}\left(z\right)-\partial_{\mu}\phi\left(y\right);\phi^{\dagger}\left(z\right)\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
