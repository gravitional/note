\begin_inset Formula 
\begin{align*}
\left(\overline{\mathcal{B}}\Gamma\mathcal{B}\right) & \equiv\overline{\mathcal{B}}_{kji}^{\alpha}\Gamma_{\alpha}^{\phantom{\alpha}\beta}\mathcal{B}_{ijk,\beta}\\
\left(\overline{\mathcal{B}}\Gamma A\mathcal{B}\right) & \equiv\overline{\mathcal{B}}_{kji}^{\alpha}\Gamma_{\alpha}^{\phantom{\alpha}\beta}A_{i\ell}\mathcal{B}_{\ell jk,\beta}\\
\left(\overline{\mathcal{B}}\Gamma\mathcal{B}A\right) & \equiv\overline{\mathcal{B}}_{kji}^{\alpha}\Gamma_{\alpha}^{\phantom{\alpha}\beta}A_{k\ell}\mathcal{B}_{ij\ell,\beta}\times\left(-\right)^{\left(i+j\right)\left(k+\ell\right)}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left(\overline{\mathcal{T}}^{\mu}\Gamma\mathcal{T}_{\mu}\right) & \equiv\overline{\mathcal{T}}_{kji,\alpha}^{\mu}\Gamma_{\phantom{\alpha}\beta}^{\alpha}\mathcal{T}_{\mu,ijk}^{\beta}\\
\left(\overline{\mathcal{T}}^{\mu}\Gamma A^{\nu}\mathcal{T}_{\mu}\right) & \equiv\overline{\mathcal{T}}_{kji,\alpha}^{\mu}\Gamma_{\phantom{\alpha}\beta}^{\alpha}A_{i\ell}^{\nu}\mathcal{T}_{\mu,\ell jk}^{\beta}\\
\left(\overline{\mathcal{B}}\Gamma A^{\mu}T_{\mu}\right) & \equiv\overline{\mathcal{B}}_{kji}^{\alpha}\Gamma_{\phantom{\alpha}\beta}^{\alpha}A_{i\ell}^{\mu}\mathcal{T}_{\mu,\ell jk}^{\beta}
\end{align*}

\end_inset


