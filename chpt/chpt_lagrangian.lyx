\begin_inset Formula 
\[
\mathcal{L}=\frac{i}{2}\left\{ b_{9}\tr\left[\overline{B}u_{\mu}\right]\sigma^{\mu\nu}\tr\left[u_{\nu}B\right]+b_{10}\tr\left[\overline{B}\left[u_{\mu},u_{\nu}\right]\sigma^{\mu\nu}B\right]+b_{11}\tr\left[\overline{B}\left\{ u_{\mu},u_{\nu}\right\} \sigma^{\mu\nu}B\right]\right\} 
\]

\end_inset

展开后的形式为，
\begin_inset Formula 
\begin{align*}
 & \overline{p}\sigma^{\mu\nu}p\left(\partial_{\mu}\pi^{+}\partial_{\nu}\pi^{-}-\partial_{\mu}\pi^{-}\partial_{\nu}\pi^{+}\right){\color{red}\frac{i}{f_{\phi}^{2}}\left(b_{10}+b_{11}\right)}\\
\text{取厄米共轭写成}\quad & \overline{p}\sigma^{\mu\nu}p\partial_{\mu}\pi^{+}\partial_{\nu}\pi^{-}{\color{red}\frac{2i}{f_{\phi}^{2}}\left(b_{10}+b_{11}\right)}
\end{align*}

\end_inset


\begin_inset Formula $\bar{\psi}\sigma^{\mu\nu}\psi$
\end_inset

