\begin_layout Subsection
十重态反常磁矩
\end_layout

\begin_layout Standard
拉氏密度为
\begin_inset Formula 
\[
\frac{3c_{T}e}{4M_{T}}\overline{T}_{\alpha}^{abc}\sigma^{\mu\nu}F_{\mu\nu}Q^{cd}T_{\alpha}^{dba}=\frac{-F_{2}^{T}}{4M_{T}}\overline{T}_{\alpha}^{abc}\sigma^{\mu\nu}F_{\mu\nu}^{a}\lambda^{a,cd}T_{\alpha}^{dba}
\]

\end_inset

展开之后的形式为：
\begin_inset Formula 
\[
\frac{e}{4M_{T}}\mathcal{F}^{\mu\nu}\overline{\Delta_{\alpha}^{++}}\sigma_{\mu\nu}\Delta^{++\alpha}{\color{red}Q_{u}3c_{T}}
\]

\end_inset

如果在动量空间，令
\begin_inset Formula $\partial^{\mu}\to-iq^{\mu}$
\end_inset

，
\begin_inset Formula $\mathcal{F}^{\mu\nu}\to-i\left(q^{\mu}\mathcal{A}^{\nu}-q^{\nu}\mathcal{A}^{\mu}\right)$
\end_inset

，则
\begin_inset Formula 
\[
ie\,\mathcal{A}^{\mu}\,\overline{\Delta_{\alpha}^{++}}\frac{\sigma_{\mu\nu}q^{\nu}}{2M_{N}}\Delta^{++\alpha}{\color{red}Q_{u}3c_{T}}
\]

\end_inset

类似上面，
\begin_inset Formula $\Delta^{++}$
\end_inset

的树图阶
\begin_inset Formula $F_{2}^{\Delta^{++}}={\color{red}Q_{u}3c_{T}}=2c_{T}$
\end_inset

。而夸克模型给出的磁矩为
\begin_inset Formula $\mu_{\Delta^{++}}=3\mu_{u}$
\end_inset

，
\begin_inset Formula $F_{2}^{\Delta^{++}}=3\mu_{u}-2$
\end_inset

。代入上面得到的关系
\begin_inset Formula 
\[
\mu_{u}=1+c_{2},\quad\mu_{d}=1-c_{1}+c_{2},\quad\mu_{u}\approx\frac{2c_{1}}{3},\quad c_{2}\approx\frac{2c_{1}}{3}-1
\]

\end_inset

有
\begin_inset Formula 
\begin{align*}
c_{T} & =\frac{1}{2}\left(3\mu_{u}-2\right)=\frac{3}{2}\left(1+c_{2}\right)-1=\frac{1}{2}\left(3c_{2}+1\right)\\
\text{近似为:}\quad c_{T} & \approx\frac{1}{2}\left(2c_{1}-2\right)=c_{1}-1
\end{align*}

\end_inset

则
\begin_inset Formula $F_{2}^{\Delta^{++}}=2c_{T}=3c_{2}+1\approx2\left(c_{1}-1\right)$
\end_inset

。
\end_layout

