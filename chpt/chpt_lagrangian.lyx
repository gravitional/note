\begin_layout Subsection
BMTA 耦合项 C
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\mathcal{C}}{2}\tr\left[\left(\varepsilon.\overline{T_{\nu}}.\left(u.\lambda^{a}.u^{\dagger}-u^{\dagger}.\lambda^{a}.u\right)\right)^{1,4}.\Theta^{\nu\mu}.B\right]+\frac{\mathcal{C}}{2}\tr\left[\overline{B}.\left(\left(u.\lambda^{a}.u^{\dagger}-u^{\dagger}.\lambda^{a}.u\right).\Theta^{\mu\nu}.T_{\nu}.\left(-\varepsilon\right)\right)^{1,4}\right]
\]

\end_inset

展开后的项类似于：
\begin_inset Formula 
\begin{align*}
 & \left[\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}\partial_{\mu}\pi^{-}-\overline{\Delta_{\nu}^{++}}\Theta^{\mu\nu}p\partial_{\mu}\pi^{+}\right]{\color{red}\frac{\mathcal{C}}{\sqrt{2}f_{\phi}}}\\
 & e\,\mathcal{A}_{\mu}\left(\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}\pi^{-}+\overline{\Delta_{\nu}^{++}}{\color{blue}\Theta^{\mu\nu}}p\pi^{+}\right){\color{red}{\color{red}i\left(Q_{u}-Q_{d}\right)}\frac{\mathcal{C}}{\sqrt{2}f_{\phi}}}\\
\text{或者写成}\quad & e\,\mathcal{A}_{\mu}\left(\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}\pi^{-}-\overline{\Delta_{\nu}^{++}}{\color{blue}\Theta^{\nu\mu}}p\pi^{+}\right){\color{red}{\color{red}i\left(Q_{u}-Q_{d}\right)}\frac{\mathcal{C}}{\sqrt{2}f_{\phi}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
其中
\begin_inset Formula $\Theta^{\mu\nu}=g^{\mu\nu}-\left(z+1/2\right)\gamma^{\mu}\gamma^{\nu}$
\end_inset

 ，取
\begin_inset Formula $z=1/2$
\end_inset

，得到
\begin_inset Formula $\Theta^{\mu\nu}=i\sigma^{\mu\nu}$
\end_inset

。
\end_layout

\begin_layout Itemize
\begin_inset Formula $\bar{\psi}\sigma^{\mu\nu}\psi$
\end_inset

是厄米的二次型，
\begin_inset Formula $\left(\bar{\psi}\sigma^{\mu\nu}\psi\right)^{\dagger}=\bar{\psi}\sigma^{\mu\nu}\psi$
\end_inset

。它的
\begin_inset Formula $C$
\end_inset

宇称为
\begin_inset Formula $-1$
\end_inset

。这里认为
\begin_inset Formula $\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}$
\end_inset

整体
\begin_inset Formula $C$
\end_inset

宇称也是
\begin_inset Formula $-1$
\end_inset

。
\begin_inset Formula $\mathcal{A}_{\mu}$
\end_inset

的
\begin_inset Formula $C$
\end_inset

宇称为
\begin_inset Formula $-1$
\end_inset

，所以
\begin_inset Formula $C$
\end_inset

宇称要求两项之间是正号。
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}$
\end_inset

和
\begin_inset Formula $\overline{\Delta_{\nu}^{++}}\Theta^{\mu\nu}p$
\end_inset

是反厄米的二次型，所以厄米性要求前面必须有个
\begin_inset Formula $i$
\end_inset

，这样整体是厄米的。
