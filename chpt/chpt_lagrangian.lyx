\begin_layout Subsection
BTM 耦合项 C
\end_layout

\begin_layout Standard
八重态–十重态–介子耦合项，
\begin_inset Formula 
\[
\mathcal{C}\tr\left\{ \left(\varepsilon\cdot\overline{T}_{\mu}.u_{\nu}\right)^{\left(1,4\right)}\cdot\left(\Theta^{\mu\nu}\cdot B\right)+\left[\overline{B}\cdot u_{\mu}\cdot\Theta^{\mu\nu}\cdot T_{\nu}\cdot\left(-\varepsilon\right)\right]^{\left(1,4\right)}\right\} .
\]

\end_inset

展开之后类似于：
\begin_inset Formula 
\begin{align*}
 & \left(\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}\partial_{\mu}\pi^{-}-\overline{\Delta_{\nu}^{++}}\Theta^{\mu\nu}p\partial_{\mu}\pi^{+}\right){\color{red}\frac{\mathcal{C}}{\sqrt{2}f_{\phi}}}\\
\text{或者写成}\quad & \left(\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}\partial_{\mu}\pi^{-}+\overline{\Delta_{\mu}^{++}}\Theta^{\mu\nu}p\partial_{\nu}\pi^{+}\right){\color{red}\frac{\mathcal{C}}{\sqrt{2}f_{\phi}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Theta^{\mu\nu}=g^{\mu\nu}-\left(z+1/2\right)\gamma^{\mu}\gamma^{\nu}$
\end_inset

 ，取
\begin_inset Formula $z=1/2$
\end_inset

，得到
\begin_inset Formula $\Theta^{\mu\nu}=i\sigma^{\mu\nu}$
\end_inset

。
\end_layout

\begin_layout Itemize
\begin_inset Formula $\bar{\psi}\sigma^{\mu\nu}\psi$
\end_inset

是厄米的二次型，
\begin_inset Formula $\left(\bar{\psi}\sigma^{\mu\nu}\psi\right)^{\dagger}=\bar{\psi}\sigma^{\mu\nu}\psi$
\end_inset

。它的
\begin_inset Formula $C$
\end_inset

宇称为
\begin_inset Formula $-1$
\end_inset

，
\begin_inset Formula $P$
\end_inset

宇称为
\begin_inset Formula $g^{\mu\nu}$
\end_inset

。这里认为
\begin_inset Formula $\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}$
\end_inset

整体的
\begin_inset Formula $C$
\end_inset

也是
\begin_inset Formula $-1$
\end_inset

。
\begin_inset Formula $C$
\end_inset

宇称要求两项之间是负号。
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overline{p}\Theta^{\mu\nu}\Delta_{\nu}^{++}$
\end_inset

和
\begin_inset Formula $\overline{\Delta_{\mu}^{++}}\Theta^{\mu\nu}p$
\end_inset

是反厄米的二次型，也要求两项之间是负号。
\end_layout

